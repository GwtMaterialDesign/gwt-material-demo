<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:demo="urn:import:gwt.material.design.demo.client.ui">
    <m:MaterialPanel>
        <m:MaterialRow addStyleNames="code">
            <m:MaterialTitle title="Text Only" />
            <m:MaterialNavBar>
                <m:MaterialBreadcrumb marginLeft="12" text="First"/>
                <m:MaterialBreadcrumb text="Second"/>
                <m:MaterialBreadcrumb text="Third"/>
            </m:MaterialNavBar>
            <demo:PrettyPre marginTop="12" addStyleNames="lang-xml">
                &emsp;&lt;m:MaterialNavBar><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb marginLeft="12" text="First"/><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb text="Second"/><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb text="Third"/><br/>
                &emsp;&lt;/m:MaterialNavBar><br/>
            </demo:PrettyPre>
        </m:MaterialRow>
    
        <m:MaterialRow addStyleNames="code">
            <m:MaterialTitle title="Only Icons" />
            <m:MaterialNavBar>
                <m:MaterialBreadcrumb marginLeft="12" iconType="POLYMER"/>
                <m:MaterialBreadcrumb iconType="STAR"/>
                <m:MaterialBreadcrumb iconType="MOTORCYCLE"/>
            </m:MaterialNavBar>
            <demo:PrettyPre marginTop="12" addStyleNames="lang-xml">
                &emsp;&lt;m:MaterialNavBar><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb marginLeft="12" iconType="POLYMER"/><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb iconType="STAR"/><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb iconType="MOTORCYCLE"/><br/>
                &lt;/m:MaterialNavBar><br/>
            </demo:PrettyPre>
        </m:MaterialRow>
    
        <m:MaterialRow addStyleNames="code">
            <m:MaterialTitle title="Text with Icons" />
            <m:MaterialNavBar>
                <m:MaterialBreadcrumb marginLeft="12" iconPosition="LEFT" iconType="POLYMER" text="First"/>
                <m:MaterialBreadcrumb iconPosition="LEFT" iconType="STAR" text="Second"/>
                <m:MaterialBreadcrumb iconPosition="LEFT" iconType="MOTORCYCLE" text="Third"/>
            </m:MaterialNavBar>
            <demo:PrettyPre marginTop="12" addStyleNames="lang-xml">
                &lt;m:MaterialNavBar><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb marginLeft="12" iconPosition="LEFT" iconType="POLYMER" text="First"/><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb iconPosition="LEFT" iconType="STAR" text="Second"/><br/>
                &emsp;&emsp;&lt;m:MaterialBreadcrumb iconPosition="LEFT" iconType="MOTORCYCLE" text="Third"/><br/>
                &lt;/m:MaterialNavBar><br/>
            </demo:PrettyPre>
        </m:MaterialRow>
    </m:MaterialPanel>
</ui:UiBinder>
