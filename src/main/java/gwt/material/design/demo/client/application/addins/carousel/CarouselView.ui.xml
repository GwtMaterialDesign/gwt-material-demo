            <demo:PrettyPre addStyleNames="lang-java">
                &emsp;btnAdd.addClickHandler(e -> { <br/>
                &emsp;MaterialImage img = new MaterialImage("//i.imgur.com/8XlWy0H.png"); <br/>
                &emsp;img.addClickHandler(e1 -> { <br/>
                &emsp;&emsp;MaterialToast.fireToast("Clicked on Image"); <br/>
                &emsp;}); <br/>
                &emsp;carouselDynamic.add(img); <br/>
                }); <br/><br/>

                btnRemove.addClickHandler(e -> { <br/>
                &emsp;carouselDynamic.remove(1); <br/>
                }); <br/>
            </demo:PrettyPre>
