            <demo:PrettyPre addStyleNames="lang-xml">
                &emsp;carouselEvents.addAfterChangeHandler(e -> {<br/>
                &emsp;MaterialToast.fireToast("AfterChangeEvent : " + e.getCurrentSlide() + " Current Slide");<br/>
                });<br/><br/>

                carouselEvents.addBeforeChangeHandler(e -> {<br/>
                &emsp;MaterialToast.fireToast("BeforeChangeEvent : " + e.getCurrentSlide() + "Current Slide " + e.getNextSlide() + " Next Slide");<br/>
                });<br/><br/>

                carouselEvents.addInitHandler(e -> {<br/>
                &emsp;MaterialToast.fireToast("InitEvent");<br/>
                });<br/><br/>

                carouselEvents.addDestroyHandler(e -> {<br/>
                &emsp;MaterialToast.fireToast("DestroyEvent");<br/>
                });<br/><br/>

                carouselEvents.addSwipeHandler(e -> {<br/>
                &emsp;MaterialToast.fireToast("SwipeEvent : " + e.getDirection() + " Direction");<br/>
                });
