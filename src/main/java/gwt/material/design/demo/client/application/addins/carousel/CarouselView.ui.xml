        <!-- Add / Remove -->
        <m:MaterialRow addStyleNames="code">
            <m:MaterialTitle title="Add / Remove Slide" description="We provided a dynamic way to add / remove slide items into the carousel." />
            <m:MaterialRow>
                <m:MaterialButton margin="4" text="Add Slide" ui:field="btnAdd" />
                <m:MaterialButton margin="4" text="Remove Slide" ui:field="btnRemove" />
                <ma:carousel.MaterialCarousel ui:field="dynamicCarousel" type="IMAGE" width="100%" textAlign="CENTER" infinite="true">
                    <ma:webp.MaterialWebpImage url="https://gwtmaterialdesign.github.io/gwt-material-demo/images/slide-1.webp" fallbackExtension="png"/>
                    <ma:webp.MaterialWebpImage url="https://gwtmaterialdesign.github.io/gwt-material-demo/images/slide-2.webp" fallbackExtension="png"/>
