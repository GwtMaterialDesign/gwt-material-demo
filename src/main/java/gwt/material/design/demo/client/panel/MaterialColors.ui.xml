	<m:MaterialPanel>
		<m:MaterialTitle title="Usage"
			description="Here is a color palette based on the material design base colors. Each of these colors is defined with a base color class and an optional lighten or darken class." />
		<m:MaterialRow>
			<m:MaterialColumn grid="s12 m3" addStyleNames="center-align">
				<m:MaterialLabel text="Background Color" fontSize="2" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s12 m9" addStyleNames="left-align">
				<m:MaterialLabel
					text="To apply a background color, just add the color name and light/darkness as a class to the element." />
				<br />
				<m:MaterialPanel color="teal lighten-2" align="center" shadow="1">
					<br />
					<m:MaterialLabel
						text="This is a Material Panel with a teal lighten-2 class" />
					<br />
				</m:MaterialPanel>
				<br />
				<pre>
&lt;m:MaterialPanel color="teal lighten-2">
&lt;m:MaterialLabel text="This is a Material Panel with a teal lighten-2 class" />
&lt;/m:MaterialPanel>
				</pre>
			</m:MaterialColumn>
			<br />
		</m:MaterialRow>
		<m:MaterialRow>
			<m:MaterialColumn grid="s12 m3" addStyleNames="center-align">
				<m:MaterialLabel text="Text Color" fontSize="2" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s12 m9" addStyleNames="left-align">
				<m:MaterialLabel text="To apply a text color, just append -text to the color class like this:" />
				<br />
				<m:MaterialPanel color="blue-text text-darken-2" align="center" shadow="1">
					<br />
					<m:MaterialLabel text="This is a Material Panel with dark blue text" />
					<br />
				</m:MaterialPanel>
				<br />
				<pre>
&lt;m:MaterialPanel color="blue-text text-darken-2" align="center">
&lt;m:MaterialLabel text="This is a Material Panel with dark blue text" />
&lt;m:MaterialPanel>
				</pre>
			</m:MaterialColumn>
			<br />
		</m:MaterialRow>
