<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:demo="urn:import:gwt.material.design.demo.client.ui">

    <ui:style>
        .block{
            padding: 15%;
        }
    </ui:style>
    <m:MaterialPanel>
        <m:MaterialPanel addStyleNames="code">
            <m:MaterialTitle title="Usage" />
            <demo:PrettyPre addStyleNames="lang-xml">
                &emsp;&lt;!-- Addin Import --><br/>
                xmlns:ma="urn:import:gwt.material.design.addins.client"<br/><br/>
                &lt;!-- Addin Usage --><br/>
                &lt;ma:stepper.MaterialStepper ui:field="stepper"><br/>
                &emsp;&lt;ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1"><br/>
                &emsp;&emsp;&lt;m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnContinue1" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnPrev1" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/><br/>
                &emsp;&lt;/ma:stepper.MaterialStep><br/>
                &lt;/ma:stepper.MaterialStepper><br/>
            </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
            <m:MaterialTitle title="Vertical Steppers" description="Vertical steppers are designed for narrow screen sizes. They are ideal for mobile." />
            <ma:stepper.MaterialStepper ui:field="stepper" axis="VERTICAL">
                <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnContinue1" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnPrev1" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
                <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 2">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnContinue2" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnPrev2" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
                <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 3">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnContinue3" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnPrev3" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
            </ma:stepper.MaterialStepper>
            <demo:PrettyPre addStyleNames="lang-xml">
                &emsp;&lt;ma:stepper.MaterialStepper ui:field="stepper"><br/>
                &emsp;&lt;ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1"><br/>
                &emsp;&emsp;&lt;m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnContinue1" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnPrev1" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/><br/>
                &emsp;&lt;/ma:stepper.MaterialStep><br/>
                &emsp;&lt;ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 1"><br/>
                &emsp;&emsp;&lt;m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnContinue2" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnPrev2" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/><br/>
                &emsp;&lt;/ma:stepper.MaterialStep><br/>
                &emsp;&lt;ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 1"><br/>
                &emsp;&emsp;&lt;m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnContinue3" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/><br/>
                &emsp;&emsp;&lt;m:MaterialButton ui:field="btnPrev3" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/><br/>
                &emsp;&lt;/ma:stepper.MaterialStep><br/>
                &lt;/ma:stepper.MaterialStepper><br/>
            </demo:PrettyPre>
            <demo:PrettyPre addStyleNames="lang-xml">
              &emsp;@UiHandler({"btnContinue1", "btnContinue2", "btnContinue3"})<br/>
              void onNextStep(ClickEvent e){<br/>
              &emsp;stepper.nextStep();<br/>
              }<br/><br/>
    
              @UiHandler({"btnPrev1", "btnPrev2", "btnPrev3"})<br/>
              void onPrevStep(ClickEvent e){<br/>
              &emsp;stepper.prevStep();<br/>
              }<br/><br/>
    
              @UiHandler("btnContinue3")<br/>
              void onFinish(ClickEvent e){<br/>
              &emsp;MaterialToast.fireToast("All done.");<br/>
              stepper.reset();<br/>
              }<br/>
            </demo:PrettyPre>
        </m:MaterialPanel>

        <m:MaterialPanel addStyleNames="code">
            <m:MaterialTitle title="Mobile" description="Open this stepper on a mobile device to test." />
            <m:MaterialPanel height="600px">
                <ma:stepper.MaterialStepper ui:field="stepperMobile" detectOrientation="true" shadow="1">
                    <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                        <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                        <m:MaterialButton ui:field="btnMobileContinue1" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                        <m:MaterialButton ui:field="btnMobilePrev1" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                    </ma:stepper.MaterialStep>
                    <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 1">
                        <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                        <m:MaterialButton ui:field="btnMobileContinue2" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                        <m:MaterialButton ui:field="btnMobilePrev2" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                    </ma:stepper.MaterialStep>
                    <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 1">
                        <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                        <m:MaterialButton ui:field="btnMobileContinue3" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                        <m:MaterialButton ui:field="btnMobilePrev3" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                    </ma:stepper.MaterialStep>
                </ma:stepper.MaterialStepper>
            </m:MaterialPanel>
            <demo:PrettyPre addStyleNames="lang-xml">
                &lt;m:MaterialStepper detectOrientation="true" shadow="1">
                    &lt;!-- Steps here -->
                &lt;/m:MaterialStepper>
            </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
            <m:MaterialTitle title="Horizontal Steppers" description="Vertical steppers are designed for narrow screen sizes. They are ideal for mobile." />
            <m:MaterialPanel height="600px" width="100%">
              <ma:stepper.MaterialStepper ui:field="stepperHori" axis="HORIZONTAL" detectOrientation="true" shadow="1">
                <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                  <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                  <m:MaterialButton ui:field="btnContinue01" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                  <m:MaterialButton ui:field="btnPrev01" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
                <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 2">
                  <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                  <m:MaterialButton ui:field="btnContinue02" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                  <m:MaterialButton ui:field="btnPrev02" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
                <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 3">
                  <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                  <m:MaterialButton ui:field="btnContinue03" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                  <m:MaterialButton ui:field="btnPrev03" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
              </ma:stepper.MaterialStepper>
            </m:MaterialPanel>
          
            <demo:PrettyPre addStyleNames="lang-xml">
              &emsp;&lt;!-- You need to contain your horizontal Stepper into a MaterialPanel to specify the width and height of your stepper --><br/>
              &lt;m:MaterialPanel height="500px" width="100%"><br/>
              &emsp;&lt;ma:stepper.MaterialStepper axis="HORIZONTAL" shadow="1"><br/>
              &emsp;&emsp;&lt;!-- Step components here --><br/>
              &emsp;&lt;/ma:stepper.MaterialStepper><br/>
              &lt;/m:MaterialPanel><br/>
            </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
            <m:MaterialTitle title="Complete Event" description="We provided an easy way to determine if the Stepper reach the last step, so we fire Complete Event." />
            <ma:stepper.MaterialStepper ui:field="stepperEvent" shadow="1">
                <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnEventContinue1" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnEventPrev1" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
                <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnEventContinue2" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnEventPrev2" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
                <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnEventContinue3" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnEventPrev3" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                </ma:stepper.MaterialStep>
            </ma:stepper.MaterialStepper>
            <demo:PrettyPre addStyleNames="lang-java">
                &emsp;@UiHandler("stepper") <br/>
                void onCompleteEvent(CompleteEvent event) { <br/>
                &emsp;event.getLastStep(); // Get the last step <br/>
                &emsp;stepper.reset(); // Reset the stepper <br/>
                }
            </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
            <m:MaterialTitle title="Card Steppers" description="You can put your Steppers inside a card component by adding shadow='1'" />
            <ma:stepper.MaterialStepper ui:field="stepperCard" shadow="1">
              <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnContinue11" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                <m:MaterialButton ui:field="btnPrev11" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
              <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 1">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnContinue12" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                <m:MaterialButton ui:field="btnPrev12" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
              <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 1">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnContinue13" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                <m:MaterialButton ui:field="btnPrev13" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
            </ma:stepper.MaterialStepper>
            <demo:PrettyPre marginTop="100" addStyleNames="lang-xml">
              &emsp;&lt;ma:stepper.MaterialStepper ui:field="stepperCard" shadow="1"><br/>
              &emsp;&lt;!-- Step components here --><br/>
              &lt;/ma:stepper.MaterialStepper><br/>
            </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
            <!-- Steppers inside dialog -->
            <m:MaterialTitle title="Dialog Steppers" description="You can put your Steppers inside a card component by adding shadow='1'"/>
            <m:MaterialButton ui:field="btnDialog" text="Open Dialog" waves="DEFAULT" textColor="WHITE"  />
            <m:MaterialDialog ui:field="dialogStepper" dismissible="false" inDuration="500" outDuration="500">
              <m:MaterialDialogContent>
                <ma:stepper.MaterialStepper ui:field="stepperDialog">
                  <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnContinue31" text="Continue to Step 2" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnPrev31" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                  </ma:stepper.MaterialStep>
                  <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnContinue32" text="Continue to Step 3" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnPrev32" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                  </ma:stepper.MaterialStep>
                  <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 1">
                    <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                    <m:MaterialButton ui:field="btnContinue33" text="Finish" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                    <m:MaterialButton ui:field="btnPrev33" text="Cancel" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
                  </ma:stepper.MaterialStep>
                </ma:stepper.MaterialStepper>
              </m:MaterialDialogContent>
            </m:MaterialDialog>
            
            <demo:PrettyPre marginTop="100" addStyleNames="lang-xml">
              &emsp;&lt;m:MaterialDialog ui:field="dialogStepper" dismissable="false" inDuration="500" outDuration="500"><br/>
              &emsp;&lt;m:MaterialDialogContent><br/>
              &emsp;&emsp;&lt;ma:stepper.MaterialStepper ui:field="stepperDialog"><br/>
              &emsp;&emsp;&emsp;&lt;!-- Step components here --><br/>
              &emsp;&emsp;&lt;/ma:stepper.MaterialStepper><br/>
              &emsp;&lt;/m:MaterialDialogContent><br/>
              &lt;/m:MaterialDialog><br/>
            </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
          <m:MaterialTitle title="Error on Steppers" description="You can specify a global stepper error by : setErrorText('Error Message'); " />
          <m:MaterialPanel height="500px" width="100%">
            <ma:stepper.MaterialStepper ui:field="stepperError" axis="HORIZONTAL" detectOrientation="true" shadow="1">
              <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnError" text="Set Error" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                <m:MaterialButton ui:field="btnSuccess" text="Continue" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
              <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 2">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnError1" text="Set Error" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                <m:MaterialButton ui:field="btnSuccess1" text="Continue" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
              <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 3">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnError2" text="Set Error" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
                <m:MaterialButton ui:field="btnSuccess2" text="Continue" grid="l4" marginTop="12" type="FLAT" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
            </ma:stepper.MaterialStepper>
          </m:MaterialPanel>
          <demo:PrettyPre marginTop="100" addStyleNames="lang-java">
            &emsp;// Setting error message<br/>
            stepper.setError("Sample error message");
          </demo:PrettyPre>
        </m:MaterialPanel>
    
        <m:MaterialPanel addStyleNames="code">
          <m:MaterialTitle title="Feedback on Steppers" description="Steppers may display a transient feedback message after a step is saved. Stepper feedback should only be used if there is a long latency between steps"/>
          <m:MaterialPanel height="500px" width="100%">
            <ma:stepper.MaterialStepper ui:field="stepperFeedback" axis="HORIZONTAL" detectOrientation="true" shadow="1">
              <ma:stepper.MaterialStep step="1" title="Name of Step 1" description="Description of Step 1">
                <m:MaterialPanel width="100%" backgroundColor="GREY_LIGHTEN_2" addStyleNames="{style.block}"/>
                <m:MaterialButton ui:field="btnContinue41" text="Continue with Feedback Loader" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
              <ma:stepper.MaterialStep step="2" title="Name of Step 2" description="Description of Step 2">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                <m:MaterialButton ui:field="btnContinue42" text="Continue with Feedback Loader" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
              <ma:stepper.MaterialStep step="3" title="Name of Step 3" description="Description of Step 3">
                <m:MaterialPanel width="100%" addStyleNames="{style.block}" backgroundColor="GREY_LIGHTEN_2"/>
                  <m:MaterialButton ui:field="btnContinue43" text="Continue with Feedback Loader" grid="l4" marginTop="12"  textColor="WHITE" waves="DEFAULT"/>
              </ma:stepper.MaterialStep>
            </ma:stepper.MaterialStepper>
          </m:MaterialPanel>
          <demo:PrettyPre marginTop="100" addStyleNames="lang-java">
              &emsp;// show feedback<br/>
              showFeedback("Feedback Message");<br/><br/>
    
              // hiding feedback<br/>
              hideFeedback();<br/>
          </demo:PrettyPre>
        </m:MaterialPanel>
    </m:MaterialPanel>
</ui:UiBinder>
